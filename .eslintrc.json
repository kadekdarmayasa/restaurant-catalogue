{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jasmine": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "no-useless-constructor": 0,
    "no-unused-expressions": 0,
    "func-names": 0,
    "no-console": 0,
    "import/no-extraneous-dependencies": 0,
    "no-param-reassign": 0,
    "operator-linebreak": 0,
    "no-restricted-globals": 0,
    "object-curly-newline": [
      "error",
      {
        "ObjectExpression": { "multiline": true },
        "ObjectPattern": { "multiline": true }
      }
    ],
    "class-methods-use-this": ["error", { "exceptMethods": ["createStarItem"] }]
  },
  "plugins": ["jasmine"]
}
